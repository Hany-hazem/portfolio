############
# API Proxy - Configuration for the Kong API gateway
############

# Change these to match your domain
API_EXTERNAL_URL=https://notworthy.vip
SUPABASE_PUBLIC_URL=https://notworthy.vip

############
# Auth
############

## General
SITE_URL=https://notworthy.vip
ADDITIONAL_REDIRECT_URLS=
JWT_EXPIRY=3600
DISABLE_SIGNUP=false
API_EXTERNAL_URL=https://notworthy.vip

## Email auth
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false

## GitHub OAuth - Add your credentials here
ENABLE_GITHUB_SIGNUP=true
GITHUB_CLIENT_ID=your_github_oauth_app_client_id
GITHUB_SECRET=your_github_oauth_app_client_secret

############
# Email
############

## Inbucket - Development only (not needed for production)
# ENABLE_EMAIL_AUTOCONFIRM should be set to true if you don't have SMTP configured

## Mailgun
SMTP_ADMIN_EMAIL=admin@notworthy.vip
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password
SMTP_SENDER_NAME=Portfolio Admin

MAILER_URLPATHS_INVITE=/auth/v1/verify
MAILER_URLPATHS_CONFIRMATION=/auth/v1/verify
MAILER_URLPATHS_RECOVERY=/auth/v1/verify
MAILER_URLPATHS_EMAIL_CHANGE=/auth/v1/verify

## Email subjects
MAILER_SUBJECTS_INVITE=You have been invited
MAILER_SUBJECTS_CONFIRMATION=Confirm Your Email
MAILER_SUBJECTS_RECOVERY=Reset Your Password
MAILER_SUBJECTS_MAGIC_LINK=Your Magic Link
MAILER_SUBJECTS_EMAIL_CHANGE=Confirm Email Change

############
# Studio - Configuration for the Dashboard
############

STUDIO_DEFAULT_ORGANIZATION=Portfolio
STUDIO_DEFAULT_PROJECT=Production

############
# Functions - Configuration for Functions
############

# NOTE: VERIFY_JWT applies to all functions. Per-function VERIFY_JWT is not supported yet.
FUNCTIONS_VERIFY_JWT=false

############
# Logs
############

LOGFLARE_LOGGER_BACKEND_API_KEY=your-super-secret-and-long-logflare-key

# Change vector.toml sinks to reflect this change
LOGFLARE_API_KEY=your-super-secret-and-long-logflare-key

############
# REST API
############

PGRST_DB_SCHEMAS=public,storage,graphql_public

############
# Image Proxy
############

IMGPROXY_ENABLE_WEBP_DETECTION=true
